@article{Newton:2001p3876,
	Abstract = {We consider the problem of inferring fold changes in gene expression
	from cDNA microarray data. Standard procedures focus on the ratio
	of measured fluorescent intensities at each spot on the microarray,
	but to do so is to ignore the fact that the variation of such ratios
	is not constant. Estimates of gene expression changes are derived
	within a simple hierarchical model that accounts for measurement
	error and fluctuations in absolute gene expression levels. Significant
	gene expression changes are identified by deriving the posterior
	odds of change within a similar model. The methods are tested via
	simulation and are applied to a panel of Escherichia coli microarrays.},
	Affiliation = {Department of Statistics, University of Wisconsin, Madison, WI 53792, USA. newton@stat.wisc.edu},
