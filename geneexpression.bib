	Journal = {J Comput Biol},
	Keywords = {Animals, Mice, Oligonucleotide Array Sequence Analysis, Probability, Models: Genetic, Databases: Nucleic Acid, Computational Biology, Software, Genomics, Chromosomes},
	Language = {eng},
	Local-Url = {file://localhost/Users/epurdom/Documents/Papers/Frey/Frey2006_J%20Comput%20Biol.pdf},
	Month = {Mar},
	Number = {2},
	Pages = {200--14},
