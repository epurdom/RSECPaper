	Doi = {10.1093/nar/gkm485},
	Journal = {Nucleic Acids Research},
	Keywords = {Humans, Cytoskeletal Proteins, RNA Precursors, Introns, Muscle: Skeletal, Transcription: Genetic, Alternative Splicing, Gene Expression Profiling, Conserved Sequence, Computational Biology, Sequence Analysis: RNA, RNA-Binding Proteins, RNA: Messenger, Base Sequence, Regulatory Sequences: Ribonucleic Acid, Myocardium, Binding Sites, Animals, Exons},
	Language = {eng},
