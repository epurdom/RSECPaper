@article{Anton:2008p496,
	Abstract = {Exon and exon+junction microarrays are promising tools for studying
	alternative splicing. Current analytical tools applied to these arrays
	lack two relevant features: the ability to predict unknown spliced
	forms and the ability to quantify the concentration of known and
	unknown isoforms. SPACE is an algorithm that has been developed to
	(1) estimate the number of different transcripts expressed under
	several conditions, (2) predict the precursor mRNA splicing structure
	and (3) quantify the transcript concentrations including unknown
	forms. The results presented here show its robustness and accuracy
	for real and simulated data.},
	Affiliation = {CEIT and TECNUN, University of Navarra, San Sebasti{\'a}n, Spain. maanton@ceit.es},
	Author = {Miguel A Anton and Dorleta Gorostiaga and Elizabeth Guruceaga and Victor Segura and Pedro Carmona-Saez and Alberto Pascual-Montano and Ruben Pio and Luis M Montuenga and Angel Rubio},
	Date-Added = {2010-08-09 17:58:34 -0700},
