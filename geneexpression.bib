@article{Kapur:2007p507,
	Abstract = {We have developed a strategy for estimating gene expression on Affymetrix
	Exon arrays. The method includes a probe-specific background correction
	and a probe selection strategy in which a subset of probes with highly
	correlated intensities across multiple samples are chosen to summarize
