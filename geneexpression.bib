	Doi = {10.1089/cmb.2006.13.579},
	Journal = {J Comput Biol},
	Keywords = {Chromosomes: Human: Pair 22, Humans, Genome: Human, Chromatin Immunoprecipitation, Sensitivity and Specificity, CpG Islands, Chromosomes: Human: Pair 21, Reproducibility of Results, Chromosome Mapping, Gene Expression Profiling, Tumor Suppressor Protein p53, Oligonucleotide Array Sequence Analysis, 5' Untranslated Regions, Protein Binding, Models: Genetic, Binding Sites},
	Language = {eng},
