	Doi = {10.1093/nar/gnh173},
	Journal = {Nucleic Acids Res},
	Keywords = {Alternative Splicing, Polymerase Chain Reaction, Organ Specificity, Promoter Regions: Genetic, Humans, Oligonucleotide Array Sequence Analysis, Terminator Regions: Genetic, Exons, Gene Expression Profiling},
	Language = {eng},
	Local-Url = {file://localhost/Users/epurdom/Documents/Papers/Le/Le2004_Nucleic%20Acids%20Res.pdf},
	Month = {Jan},
	Number = {22},
	Pages = {e180},
