	Journal = {Genome Research},
	Keywords = {Humans, Sequence Alignment, Models: Genetic, Cluster Analysis, Genome: Human, Exons, Expressed Sequence Tags, RNA: Messenger, Mice, Animals, Rats, Algorithms, Alternative Splicing, Chromosome Mapping},
	Language = {eng},
