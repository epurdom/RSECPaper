@article{Efron:2001p3687,
	Abstract = {Microarrays are a novel technology that facilitates the simultaneous
	measurement of thousands of gene expression levels. A typical microarray
	experiment can produce millions of data points, raising serious problems
	of data reduction, and simultaneous inference. We consider one such
	experiment in which oligonucleotide arrays were employed to assess
	the genetic effects of ionizing radiation on seven thousand human
	genes. A simple nonparametric empirical Bayes model is introduced,
	which is used to guide the efficient reduction of the data to a single
	summary statistic per gene, and also to make simultaneous inferences
	concerning which genes were affected by the radiation. Although our
	focus is on one specific experiment, the proposed methods can be
	applied quite generally. The empirical Bayes inferences are closely
	related to the frequentist false discovery rate (FDR) criterion.},
	Author = {Bradley Efron and Robert Tibshirani and John Storey and Virginia Tusher},
